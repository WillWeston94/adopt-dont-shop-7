<h1>New Application</h1>
<%= form_with url: "/applications", method: :post, data: { turbo: false} do |f| %>
  <%= f.label :applicant_name, "Name" %></br>
  <%= f.text_field :applicant_name %></br></br>

  <%= f.label :street_address, "Street Address "%></br>
  <%= f.text_field :street_address %> </br></br>

  <%= f.label :city %></br>
  <%= f.text_field :city %></br></br>

  <%= f.label :state %></br>
  <%= f.text_field :state %></br></br>

  <%= f.label :zip_code, "Zip Code" %></br>
  <%= f.text_field :zip_code %></br></br>

  <%= f.label :description, "Description of why I would make a good home" %></br>
  <%= f.text_area :description %></br></br>

  <%= f.submit "Submit" %>
<% end %>




 
